class Solution {
public:
    void rotate(vector<int>& nums, int k) {
        if(!nums.empty()){
        for(int i=0;i<k;i++)
        {
            int temp=0;
            temp=nums[nums.size()-1];
            for(int j=0;j<nums.size()-1;j++)
            {
                nums[nums.size()-1-j]=nums[nums.size()-2-j];
            }
             nums[0]  =temp; 
        }
    }}
};
